# Server Configuration
PORT=8082
NODE_ENV=development

# Database Configuration
DB_HOST=localhost
DB_USER=root
DB_PASSWORD=photos
DB_NAME=mydb
DB_CONNECTION_LIMIT=10

# Cache Configuration
CACHE_CLEAR_INTERVAL=600000
ITEMS_PER_PAGE=20

# File Upload Configuration
MAX_FILE_SIZE=104857600

# Image Processing
THUMBNAIL_WIDTH=200
THUMBNAIL_HEIGHT=200
THUMBNAIL_QUALITY=80
GENERATE_WEBP=true

# Thumbnail Storage
# Separate directory for generated thumbnails (outside media files location)
THUMBNAIL_DIR=./temp-pic/thumbnails
THUMBNAIL_CACHE_ENABLED=true
